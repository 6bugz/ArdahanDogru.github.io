{
  "expo": {
    "name": "Fakenstein",
    "slug": "Fakenstein",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.SixBugs.Fakenstein",
      "versionCode": 1
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Fakenstein accesses your photos to let you share them with your friends.",
          "savePhotosPermission": "Allow Fakenstein to save photos to your phone."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Fakenstein to access your photos.",
          "savePhotosPermission": "Allow Fakenstein to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ]
  }
}
